{
    "env": {
        "browser": false,
        "commonjs": false,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        },
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "no-const-assign": "error",
        "no-this-before-super": "error",
        "no-undef": "error",
        "no-unreachable": "error",
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
        "constructor-super": "error",
        "valid-typeof": "error",
        "curly": "warn",
        "no-trailing-spaces": "warn",
        "require-await": "error",
        "semi": "warn"
    }
}